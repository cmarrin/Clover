struct Simple
{

struct A
{
    int8 b = 12;
    int16 c = 24;
};

const A arr[ ] = { 8, 9, 10, 11, 12, 13 };

int8 xxx = 9;

function int32 main()
{
    A a;
    
    core.printf(" v1=%d, v2=%d\n", arr[1].b, arr[2].c);
